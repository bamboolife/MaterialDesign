<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sundy.exercise">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".aty.BehaviorActivity"></activity>
        <activity android:name=".aty.TextInputActivity" />
        <activity android:name=".aty.SwipeDismissBehaviorActivity" />
        <activity android:name=".aty.CollapsingToolbarLayoutActivity" />
        <activity android:name=".aty.FloatActionButtonActivity" />
        <activity android:name=".aty.CoordinatorLayoutActivity" />
        <activity android:name=".aty.BottomDialogActivity" />
        <activity android:name=".aty.BottomDialogFragmentActivity" />
        <activity android:name=".aty.BottomSheetDialogActivity" />
        <activity android:name=".aty.ToolbarActivity" />
        <activity android:name=".DetailActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>